<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.to.markdownnote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KotlinMD">
        <activity android:name="com.to.markdownnote.TopActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.to.markdownnote.EditorActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.to.markdownnote.TopActivity" />
        </activity>
    </application>

</manifest>